<launch>
  <!-- Run Cartographer Node -->
  <machine name="test_host" address="192.168.3.218" env-loader="/opt/ros/kinetic/setup.bash;/home/dysun/ws/install_isolated/setup.sh" user="dysun"/>
  <param name="/use_sim_time" value="false" />
  <node machine="test_host" name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory
              $(find derobotee)/cfg
          -configuration_basename diff.lua"
      output="screen">
    <!--remap from="imu" to="imu/data" /-->
    <!--remap from="odom" to="odometry/filtered" /-->
  </node>

  <node machine="test_host" name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.05" />
  <rosparam file="$(find derobotee)/param/robot.yaml" command="load" ns="derobotee" />
  <rosparam file="$(find derobotee)/param/motor.yaml" command="load" ns="motor" />
  <include file="$(find derobotee)/launch/includes/run.launch.xml"/>

</launch>
